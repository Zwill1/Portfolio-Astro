---
import MainHead from "./MainHead.astro";


interface Meta {
 title: string;
 description: string;
}

const {
	title = "Title",
	description = "description for the website",
} = Astro.props as Meta;

---


<!doctype html>
<html lang="en">
	<MainHead title={title} description={description} />
	<body>
		<slot />
	</body>
</html>

